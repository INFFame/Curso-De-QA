package com.account.test;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.BeforeAll;
import org.junit.jupiter.api.Test;

import com.account.Account;

class Account_Test {
	
	static Account account1;

	@BeforeAll
	static void setUpBeforeClass() throws Exception {
		account1 = new Account(100, "abc123");
	}

	@Test
	void verifyCorrectAccountNumber_Test() {
		System.out.println("Account number is: " + account1.getAccountNumber());
		assertEquals("abc123", account1.getAccountNumber(), "Failed: The account number is not correct.");
	}

}
